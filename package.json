{
  "name": "sleekspriter",
  "productName": "SleekSpriter",
  "version": "0.2.35",
  "description": "A pixel art editor",
  "private": true,
  "main": ".webpack/main",
  "author": "Volker Wieban",
  "license": "All rights reserved",
  "scripts": {
    "start": "concurrently npm:web:start npm:electron:start",
    "build": "yarn web:build && yarn electron:make",
    "prettier": "prettier-eslint --list-different \"./{src,tests}/**/*.js\"",
    "prettier:fix": "prettier-eslint --write \"./{src,tests}/**/*.js\"",
    "lint": "eslint \"./{src,tests}/**/*.js\"",
    "lint:fix": "eslint \"./{src,tests}/**/*.js\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest -w 1",
    "web:start": "webpack-dev-server --config ./webpack/webpack.config.development.js",
    "web:build": "webpack --config ./webpack/webpack.config.browser.js; ./scripts/update_homepage.sh",
    "electron:start": "electron-forge start",
    "electron:make": "yarn electron:make:mac && yarn electron:make:windows",
    "electron:make:mac": "electron-forge make --platform=darwin",
    "electron:make:windows": "electron-forge make --platform=win32 --arch=ia32,x64",
    "electron:make:linux": "electron-forge make --platform=linux --arch=ia32,x64",
    "electron:publish": "electron-forge publish",
    "electron:package": "electron-forge package",
    "release": "./scripts/release.sh"
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.5.0",
    "@welldone-software/why-did-you-render": "^3.6.0",
    "classnames": "^2.2.6",
    "crypto-js": "^3.1.9-1",
    "electron-squirrel-startup": "^1.0.0",
    "gif.js": "^0.2.0",
    "image-size": "^0.8.3",
    "image-type": "^4.1.0",
    "lodash": "^4.17.15",
    "mousetrap": "^1.6.5",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-color": "^2.18.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-undo": "^1.0.0",
    "sprout-data": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-do-expressions": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-proposal-function-sent": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@electron-forge/cli": "6.0.0-beta.47",
    "@electron-forge/maker-deb": "6.0.0-beta.47",
    "@electron-forge/maker-dmg": "^6.0.0-beta.47",
    "@electron-forge/maker-rpm": "6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "6.0.0-beta.47",
    "@electron-forge/maker-zip": "6.0.0-beta.47",
    "@electron-forge/plugin-webpack": "6.0.0-beta.47",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "css-loader": "^3.4.2",
    "electron": "7.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest-cli": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "json-loader": "^0.5.7",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "node-loader": "^0.6.0",
    "prettier-eslint-cli": "^5.0.0",
    "react-test-renderer": "16.12.0",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "worker-loader": "^2.0.0",
    "yaml-loader": "^0.5.0"
  }
}
